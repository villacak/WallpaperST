<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="match_parent"
	android:layout_height="match_parent">

	<TextView android:text="@string/galleryExplanation"
		android:id="@+id/gallerytextView1" android:layout_width="wrap_content"
		android:layout_height="wrap_content" />


	<TextView android:text="" android:id="@+id/gallerytextView1"
		android:layout_width="wrap_content" android:layout_height="wrap_content" />


	<LinearLayout android:layout_height="wrap_content"
		android:layout_width="match_parent">

		<SeekBar android:id="@+id/seekBar1" android:layout_height="wrap_content"
			android:layout_width="wrap_content" android:max="100"
			android:progress="1" android:paddingLeft="25px" android:paddingRight="15px"
			android:layout_weight="1" />

		<EditText android:id="@+id/gallerytextscreensize"
			android:layout_width="wrap_content" android:layout_height="wrap_content" android:inputType="numberDecimal"  android:enabled="false" android:layout_weight="0.21"/>

	</LinearLayout>

	<TextView android:text="" android:id="@+id/gallerytextView1"
		android:layout_width="wrap_content" android:layout_height="wrap_content" />



	<ToggleButton android:text="Landscape" android:id="@+id/toggleButton1"
		android:textOn="Activated Landscape" android:textOff="Deactivated Landscape"
		android:hint="Disable just portrait. Enabled, both orientation"
		android:layout_width="match_parent" android:layout_height="wrap_content"
		android:paddingLeft="20px" android:paddingRight="20px" />

	<TextView android:text="" android:id="@+id/gallerytextView1"
		android:layout_width="wrap_content" android:layout_height="wrap_content" />

	<Button android:text="Reset to Default" android:id="@+id/button1"
		android:layout_height="wrap_content" android:layout_width="match_parent"
		android:paddingLeft="40px" android:paddingRight="40px" />


</LinearLayout>
